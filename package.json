{
    "name": "@kynesyslabs/demosdk",
    "version": "2.2.69",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "author": "Kynesys Labs",
    "license": "MIT",
    "files": [
        "build/",
        "!build/tests"
    ],
    "exports": {
        ".": "./build/index.js",
        "./types": "./build/types/index.js",
        "./websdk": "./build/websdk/index.js",
        "./demoswork": "./build/demoswork/index.js",
        "./xmcore": "./build/multichain/core/index.js",
        "./xm-websdk": "./build/multichain/websdk/index.js",
        "./xm-localsdk": "./build/multichain/localsdk/index.js",
        "./wallet": "./build/wallet/index.js",
        "./abstraction": "./build/abstraction/index.js",
        "./l2ps": "./build/l2ps/index.js",
        "./utils": "./build/utils/index.js",
        "./encryption": "./build/encryption/index.js",
        "./bridge": "./build/bridge/index.js"
    },
    "scripts": {
        "build": "rm -rf build && tsc --skipLibCheck && resolve-tspaths && mv build/src/* build/ && rm -rf build/src",
        "publish": "yarn npm publish",
        "typedoc": "typedoc",
        "test": "jest",
        "test:multichain": "rm -rf build && jest --testMatch '**/tests/**/{coin,evm}*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose",
        "test:tx": "rm -rf build && jest --testMatch '**/tests/multichain**/fulltx*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose",
        "test:demoswork": "rm -rf build && jest --testMatch '**/tests/**/demoswork*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose",
        "test:identities": "rm -rf build && jest --testMatch '**/tests/**/identities.spec*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose",
        "test:identities:pqc": "rm -rf build && jest --testMatch '**/tests/**/identities.pqc.spec*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose",
        "test:identities:web2": "rm -rf build && jest --testMatch '**/tests/**/identities.web2*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose --setupFiles dotenv/config",
        "setup:pre-push": "cp .github/hooks/pre-push .git/hooks/pre-push && chmod +x .git/hooks/pre-push && echo \"Pre-push hook installed\"",
        "test:native": "rm -rf build && jest --testMatch '**/tests/native*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose",
        "test:web2": "rm -rf build && jest --testMatch '**/tests/web2*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose",
        "test:demos": "rm -rf build && jest --testMatch '**/newdemos.spec.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose",
        "test:rubic-service": "rm -rf build && jest --testMatch '**/tests/**/rubic*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template* --verbose",
        "test:btc": "rm -rf build && jest --testMatch '**/tests/multichain**/bitcoin*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose",
        "test:pqc": "rm -rf build && jest --testMatch '**/tests/encryption/pqc*.ts' --verbose",
        "test:pcq": "yarn test:pqc"
    },
    "dependencies": {
        "@bitcoinerlab/secp256k1": "^1.2.0",
        "@cosmjs/proto-signing": "^0.32.3",
        "@cosmjs/stargate": "^0.32.3",
        "@metaplex-foundation/js": "^0.20.1",
        "@multiversx/sdk-core": "^13.16.0",
        "@multiversx/sdk-extension-provider": "^3.0.0",
        "@multiversx/sdk-network-providers": "^2.4.3",
        "@multiversx/sdk-wallet": "^4.4.0",
        "@noble/hashes": "^1.8.0",
        "@noble/post-quantum": "^0.4.0",
        "@project-serum/anchor": "^0.26.0",
        "@roamhq/wrtc": "^0.8.0",
        "@simplewebauthn/browser": "^11.0.0",
        "@simplewebauthn/server": "^11.0.0",
        "@solana/buffer-layout": "^4.0.1",
        "@solana/web3.js": "1.98.0",
        "@the-convocation/twitter-scraper": "^0.16.6",
        "@ton/core": "^0.56.3",
        "@ton/crypto": "^3.2.0",
        "@ton/ton": "^13.11.2",
        "@types/simple-peer": "^9.11.8",
        "axios": "^1.7.4",
        "big-integer": "^1.6.52",
        "bignumber.js": "^9.1.2",
        "bip32": "^5.0.0-rc.0",
        "bitcoinjs-lib": "^6.1.7",
        "bitcoinjs-message": "^2.2.0",
        "bs58": "^5.0.0",
        "buffer": "^6.0.3",
        "dotenv": "^16.4.7",
        "ecpair": "^3.0.0",
        "ethers": "^6.11.1",
        "falcon-js": "^1.0.0",
        "falcon-sign": "^1.0.4",
        "js-sha3": "^0.9.3",
        "libsodium-wrappers-sumo": "^0.7.15",
        "lodash": "^4.17.21",
        "mlkem": "^2.3.1",
        "near-api-js": "^4.0.4",
        "node-forge": "^1.3.1",
        "node-seal": "^5.1.3",
        "ntru": "^4.0.4",
        "pqcrypto": "^1.0.1",
        "protobufjs": "^7.4.0",
        "ripple-keypairs": "^2.0.0",
        "rubic-sdk": "^5.52.6",
        "simple-peer": "^9.11.1",
        "socket.io-client": "^4.7.2",
        "sphincs": "^3.0.4",
        "superdilithium": "^2.0.6",
        "web3": "^4.9.0",
        "xrpl": "^3.1.0"
    },
    "devDependencies": {
        "@orbs-network/ton-access": "^2.3.3",
        "@types/cors": "^2.8.17",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.4",
        "@types/node-forge": "^1.3.11",
        "jest": "^29.7.0",
        "resolve-tspaths": "^0.8.18",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typedoc": "^0.26.6",
        "typescript": "^5.4.3"
    },
    "packageManager": "yarn@3.8.7"
}
