{
    "name": "@kynesyslabs/demosdk",
    "version": "1.0.24",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "author": "Kynesys Labs",
    "license": "MIT",
    "files": [
        "build/",
        "!build/tests"
    ],
    "exports": {
        ".": "./build/index.js",
        "./types": "./build/types/index.js",
        "./websdk": "./build/websdk/index.js",
        "./xm-websdk": "./build/multichain/websdk/index.js",
        "./xm-localsdk": "./build/multichain/localsdk/index.js"
    },
    "scripts": {
        "build": "rm -rf build && tsc --skipLibCheck && resolve-tspaths && mv build/src/* build/ && rm -rf build/src",
        "publish": "yarn npm publish",
        "test:multichain": "rm -rf build && jest --testMatch '**/tests/**/*.ts' --testPathIgnorePatterns **/tests/**/chainProvider* **/tests/utils/* **/tests/**/template*  --verbose"
    },
    "dependencies": {
        "@cosmjs/proto-signing": "^0.32.3",
        "@cosmjs/stargate": "^0.32.3",
        "@multiversx/sdk-core": "^13.0.0",
        "@multiversx/sdk-extension-provider": "^3.0.0",
        "@multiversx/sdk-network-providers": "^2.4.3",
        "@multiversx/sdk-wallet": "^4.4.0",
        "@solana/web3.js": "^1.91.7",
        "@types/rijndael-js": "^1.0.2",
        "argon2": "^0.40.1",
        "big-integer": "^1.6.52",
        "bs58": "^5.0.0",
        "ethers": "^6.11.1",
        "mceliece-nist": "^4.0.1",
        "node-forge": "^1.3.1",
        "node-seal": "^5.1.3",
        "rijndael-js": "^2.0.0",
        "socket.io-client": "^4.7.2",
        "superdilithium": "^2.0.6",
        "xrpl": "^3.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.4",
        "@types/node-forge": "^1.3.11",
        "jest": "^29.7.0",
        "resolve-tspaths": "^0.8.18",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.3"
    },
    "packageManager": "yarn@3.8.0"
}
