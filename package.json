{
    "name": "@kynesyslabs/demos",
    "version": "1.0.0",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "private": true,
    "author": "Kynesys Labs",
    "license": "MIT",
    "files": [
        "build/",
        "build/tests/",
        "*.md"
    ],
    "exports": {
        ".": "./build/index.js",
        "./types/*": "./build/types/*",
        "./xm/*": "./build/multichain/*"
    },
    "scripts": {
        "build": "rm -rf build && tsc && resolve-tspaths && mv build/src/* build/ && rm -rf build/src",
        "publish": "yarn npm publish",
        "test:multichain": "jest --testMatch '**/tests/**/*.ts' --testPathIgnorePatterns tests/**/chainProvider* tests/utils/* tests/**/template*  --verbose"
    },
    "dependencies": {
        "@cosmjs/proto-signing": "^0.32.3",
        "@cosmjs/stargate": "^0.32.3",
        "@multiversx/sdk-core": "^13.0.0",
        "@multiversx/sdk-extension-provider": "^3.0.0",
        "@multiversx/sdk-network-providers": "^2.4.3",
        "@multiversx/sdk-wallet": "^4.4.0",
        "ethers": "^6.11.1",
        "node-forge": "^1.3.1",
        "xrpl": "^3.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.4",
        "@types/node-forge": "^1.3.11",
        "jest": "^29.7.0",
        "resolve-tspaths": "^0.8.18",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.3"
    },
    "packageManager": "yarn@3.8.0"
}
